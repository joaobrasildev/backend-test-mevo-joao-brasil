generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Operation {
  id      Int     @default(autoincrement()) @id
  from    String
  to      String
  amount  Int
  suspect Boolean
}

model OperationFailedHistory {
  id     Int     @default(autoincrement()) @id
  from   String
  to     String
  amount Int
  reason String
}